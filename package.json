{
  "name": "njsts-libra-offers-api",
  "private": true,
  "version": "0.22.0",
  "description": "Libra - Offers API for Node.js express apps with TypeScript - based on API REST Starter (@2018 Pawel Sasin)",
  "bugs": {
    "email": "pwnpsasin@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.pwn.net.pl/Libra/njsts-libra-offers-api"
  },
  "author": "Pawel Sasin",
  "contributors": [
    "Marcin Tokarski"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run clean && npm run build-sass && npm run build-ts && npm run redoc:bundle && npm run copy-static-assets",
    "doc:v1": "typedoc --out dist/public/docs/api/v1 --ignoreCompilerErrors src/v1",
    "doc:v2": "typedoc --out dist/public/docs/api/v2 --ignoreCompilerErrors src/v2",
    "doc:common": "typedoc --out dist/public/docs/common --ignoreCompilerErrors src/common",
    "doc": "run-p doc:common doc:v1",
    "serve": "node -r ./tsconfig-paths-bootstrap.js dist/server.js",
    "synerise-build": "node -r ./tsconfig-paths-bootstrap.js dist/scripts/synerise/index.js",
    "watch-node": "nodemon -r ./tsconfig-paths-bootstrap.js dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --color",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:verb": "jest --verbose",
    "test:debug": "node --inspect-brk -r tsonfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "watch-test": "npm run test -- --watchAll",
    "clean": "rimraf dist",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect -r ./tsconfig-paths-bootstrap.js dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "todo": "leasot 'src/**/*.ts'",
    "todo-ci": "leasot -x --reporter markdown 'src/**/*.ts' > TODO.md",
    "postinstall": "npm run githooks:copy && npm run githooks:chmod ",
    "format:check": "prettier --write --config ./.prettierrc --list-different \"src/**/*{.ts,.json,.scss}\"",
    "githooks:copy": "copyfiles -u 2 tools/githooks/* ./.git/hooks/",
    "githooks:chmod": "chmod +x ./.git/hooks/* || true",
    "redoc:bundle": "npx redoc-cli build --options='{\"downloadFileName\": \"swagger.json\"}' ./src/base-swagger.json && mv redoc-static.html src/views/api.html",
    "ts-script": "ts-node -r tsconfig-paths/register"
  },
  "dependencies": {
    "@types/node-schedule": "^1.3.0",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
